           MOVE SUBSTRING(WS-RULES-SECTION , WS-I + 2) TO WS-RULES-SECTION.

       BUILD-PATTERN.
           MOVE '^' TO WS-PATTERN.
           PERFORM GET-RULE-PATTERN("0").
           STRING WS-PATTERN, '$' DELIMITED BY SIZE INTO WS-PATTERN.

       GET-RULE-PATTERN.
           USING WS-RULE-NUM
           MOVE SPACES TO WS-RULE-VALUE.
           PERFORM VARYING WS-I FROM 1 BY 1 UNTIL
               WS-RULE-NUM = SUBSTRING(WS-RULES-SECTION (WS-I:3) , 1 , 3)
           END-PERFORM.
           MOVE SUBSTRING(WS-RULES-SECTION (WS-I:1024) , 5 , 1024) TO WS-RULE-VALUE.

           IF WS-RULE-VALUE(1:1) = '"'
               MOVE WS-RULE-VALUE(2:1) TO WS-RULE-CHAR
               STRING WS-PATTERN, WS-RULE-CHAR DELIMITED BY SIZE INTO WS-PATTERN
           ELSE
               MOVE SPACES TO WS-TEMP-PATTERN.
               PERFORM VARYING WS-J FROM 1 BY 1 UNTIL
                   SUBSTRING(WS-RULE-VALUE (WS-J:2) , 1 , 2) = ' |'
               END-PERFORM.
               IF WS-J > 1
                   MOVE SUBSTRING(WS-RULE-VALUE , 1 , WS-J - 1) TO WS-RULE-PART
                   STRING WS-TEMP-PATTERN, '(' DELIMITED BY SIZE INTO WS-TEMP-PATTERN
                   PERFORM VARYING WS-K FROM 1 BY 1 UNTIL
                       WS-RULE-PART(WS-K:1) = SPACE
                   END-PERFORM.
                   MOVE SUBSTRING(WS-RULE-PART , 1 , WS-K - 1) TO WS-SUB-RULE-NUM
                   PERFORM GET-RULE-PATTERN(WS-SUB-RULE-NUM)
                   STRING WS-TEMP-PATTERN, '|' DELIMITED BY SIZE INTO WS-TEMP-PATTERN
                   MOVE SUBSTRING(WS-RULE-PART , WS-K + 1) TO WS-RULE-PART
                   PERFORM VARYING WS-K FROM 1 BY 1 UNTIL
                       WS-RULE-PART(WS-K:1) = SPACE
                   END-PERFORM.
                   MOVE SUBSTRING(WS-RULE-PART , 1 , WS-K - 1) TO WS-SUB-RULE-NUM
                   PERFORM GET-RULE-PATTERN(WS-SUB-RULE-NUM)
                   STRING WS-TEMP-PATTERN, ')' DELIMITED BY SIZE INTO WS-TEMP-PATTERN
               ELSE
                   MOVE WS-RULE-VALUE TO WS-RULE-PART
                   STRING WS-TEMP-PATTERN, '(' DELIMITED BY SIZE INTO WS-TEMP-PATTERN
                   PERFORM VARYING WS-K FROM 1 BY 1 UNTIL
                       WS-RULE-PART(WS-K:1) = SPACE
                   END-PERFORM.
